        "laravel-mix": "^1.7.2",
        "lodash": "^4.17.10",
        "morris.js": "^0.5.0",
        "raphael": "^2.2.7",
        "vue": "^2.5.17",
        "vue-affix": "^0.2.4",
        "vue2-google-maps": "^0.8.12"
    },
